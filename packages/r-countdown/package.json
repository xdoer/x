    {
      "path": "dist/r-countdown.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "count-it-down-timer": "1.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "react": "^17.0.2",
    "tslib": "^2.2.0"
